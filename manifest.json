{
  "manifest_version": 2,
  "name": "Notion RPG Time Tracker",
  "version": "1.0",
  "description": "Extend the fabulous notion.so by an in-game time tracker and the possibility to trigger reminders for in-game dates",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.notion.so/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}
