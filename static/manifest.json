{
  "manifest_version": 2,
  "name": "Notion RPG Day Tracker",
  "version": "{{version}}",
  "description": "Extend the fabulous Notion by an in-game day tracker with reminders.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://*.notion.so/*"
      ],
      "css": [
        "styles/content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}
